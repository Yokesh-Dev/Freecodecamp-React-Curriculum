import {useState} from 'react';
Â 
export function Profile() {
const [user, setUser] =useState({name:"Dimitri", age:26 , city:"Congo" ,})

const handleChange =(e) => {
const {name,value} =e.target;

setUser((prevUser) =>({...prevUser, [name]:value }))
}


return(
<div>
<h1>User Profile </h1>
<h2>Name:{user.name}</h2>
<h2>Age:{user.age}</h2>
<h2>City:{user.city}</h2>

<h4>Updated User Name</h4>
<input type="text" name="name" input ={user.name} onChange={handleChange} />

<h4>Update user Age</h4>
<input type="text" name="age" input={user.age} onChange={handleChange} />

<h4>Update user City</h4>
<input type="text" name="city" input={user.city} onChange={handleChange} />

</div>
)
}

