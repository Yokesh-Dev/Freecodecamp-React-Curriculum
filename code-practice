import {useState} from 'react;

export function Counter() {
const [counter,setCounter] =useState(0);
return (
<div>
<h1>Counter App </h1>
<p>{Counter}</p>
<button onClick = {() => setCounter(count+1)}> + >/button>
<button onclick ={() => setCounter(count-1)}> - </button>

</div>
)}

